Private Sub PS_PP990_MTX01( _
                                        ByVal pBPLID As String, _
                                        ByVal pTeamCode As String, _
                                        ByVal pRspCode As String, _
                                        ByVal pClsCode As String, _
                                        ByVal pCardType As String, _
                                        ByVal pItemType As String, _
                                        ByVal pWCYN As String, _
                                        ByVal pDateStd As String, _
                                        ByVal pFrDt As String, _
                                        ByVal pToDt As String, _
                                        ByVal pItemCode As String, _
                                        ByVal pCpCode As String, _
                                        ByVal pCpName As String, _
                                        ByVal pOrdGbn As String _
                                    )
On Error GoTo PS_PP990_MTX01_Error

    Call oForm01.Freeze(True)
    
    Dim i As Long
    Dim sQry As String
    Dim ErrNum As String
    
    oForm01.Items("CpCode").Specific.VALUE = pCpCode
    oForm01.Items("CpName").Specific.VALUE = pCpName
    
    Dim ProgBar01 As SAPbouiCOM.ProgressBar
    Set ProgBar01 = Sbo_Application.StatusBar.CreateProgressBar("조회 중...", 100, False)
    
    sQry = "         EXEC [PS_PP990_01] '"
    sQry = sQry & pBPLID & "','" '사업장
    sQry = sQry & pTeamCode & "','" '팀
    sQry = sQry & pRspCode & "','" '담당
    sQry = sQry & pClsCode & "','" '반
    sQry = sQry & pCardType & "','" '거래처구분
    sQry = sQry & pItemType & "','" '품목구분
    sQry = sQry & pWCYN & "','" '생산완료여부
    sQry = sQry & pDateStd & "','" '일자기준
    sQry = sQry & pFrDt & "','" '기간(Fr)
    sQry = sQry & pToDt & "','" '기간(To)
    sQry = sQry & pItemCode & "','" '품목코드(작번)
    sQry = sQry & pCpCode & "','" '공정
    sQry = sQry & pOrdGbn & "'" '작업구분
    
    oGrid01.DataTable.Clear

    Call oForm01.DataSources.DataTables.Item("DataTable").ExecuteQuery(sQry)
    oGrid01.DataTable = oForm01.DataSources.DataTables.Item("DataTable")
    
    oGrid01.Columns(6).RightJustified = True
    oGrid01.Columns(7).RightJustified = True
    oGrid01.Columns(10).RightJustified = True
    oGrid01.Columns(11).RightJustified = True
        
    If oGrid01.Rows.Count = 0 Then
        ErrNum = 1
        GoTo PS_PP990_MTX01_Error
    End If
        
    oGrid01.AutoResizeColumns
    oForm01.Update
    
    ProgBar01.VALUE = 100
    ProgBar01.Stop
    Set ProgBar01 = Nothing

    Call oForm01.Freeze(False)
    Exit Sub

PS_PP990_MTX01_Error:

    ProgBar01.VALUE = 100
    ProgBar01.Stop
    Set ProgBar01 = Nothing

    Call oForm01.Freeze(False)
    
    If ErrNum = 1 Then
        Call MDC_Com.MDC_GF_Message("결과가 존재하지 않습니다.", "E")
    Else
        Call Sbo_Application.SetStatusBarMessage("PS_PP990_MTX01_Error: " & Err.Number & " - " & Err.Description, bmt_Short, True)
    End If
    
End Sub

