Public Sub LoadForm(Optional ByVal oFromDocEntry01 As String)
On Error GoTo LoadForm_Error
    Dim i As Long
    Dim oInnerXml01 As String
    Dim oXmlDoc01 As New MSXML2.DOMDocument
    
    oXmlDoc01.Load (SubMain.ShareFolderPath & "ScreenPS\PS_QM430.srf")
    oXmlDoc01.selectSingleNode("Application/forms/action/form/@uid").nodeValue = oXmlDoc01.selectSingleNode("Application/forms/action/form/@uid").nodeValue & "_" & (GetTotalFormsCount)
    oXmlDoc01.selectSingleNode("Application/forms/action/form/@top").nodeValue = _
            oXmlDoc01.selectSingleNode("Application/forms/action/form/@top").nodeValue + (GetCurrentFormsCount * 10)
    oXmlDoc01.selectSingleNode("Application/forms/action/form/@left").nodeValue = _
            oXmlDoc01.selectSingleNode("Application/forms/action/form/@left").nodeValue + (GetCurrentFormsCount * 10)
    
    '매트릭스의 타이틀높이와 셀높이를 고정
    For i = 1 To (oXmlDoc01.selectNodes("Application/forms/action/form/items/action/item/specific/@titleHeight").length)
        oXmlDoc01.selectNodes("Application/forms/action/form/items/action/item/specific/@titleHeight").Item(i - 1).nodeValue = 20
        oXmlDoc01.selectNodes("Application/forms/action/form/items/action/item/specific/@cellHeight").Item(i - 1).nodeValue = 16
    Next
    
    oFormUniqueID01 = "PS_QM430_" & GetTotalFormsCount
    AddForms Me, oFormUniqueID01 '//폼추가
    Sbo_Application.LoadBatchActions oXmlDoc01.xml
    '폼 할당
     
    Set oForm01 = Sbo_Application.Forms.Item(oFormUniqueID01)
    
    oForm01.SupportedModes = -1
    oForm01.Mode = fm_ADD_MODE
    '//oForm01.DataBrowser.BrowseBy="DocEntry" '//UDO방식일때
        
    oForm01.Freeze True
    Call PS_QM430_CreateItems
'    Call PS_QM430_ComboBox_Setting
'    Call PS_QM430_CF_ChooseFromList
'    Call PS_QM430_EnableMenus
'    Call PS_QM430_SetDocument(oFromDocEntry01)
'    Call PS_QM430_FormResize

'    Call PS_QM430_Add_MatrixRow01(0, True)
    
    oForm01.EnableMenu ("1283"), False        '// 삭제
    oForm01.EnableMenu ("1286"), False        '// 닫기
    oForm01.EnableMenu ("1287"), False        '// 복제
    oForm01.EnableMenu ("1285"), False        '// 복원
    oForm01.EnableMenu ("1284"), True         '// 취소
    oForm01.EnableMenu ("1293"), False         '// 행삭제
    oForm01.EnableMenu ("1281"), False
    oForm01.EnableMenu ("1282"), True
        
    oForm01.Update
    oForm01.Freeze False
    
    '검사일자
    oForm01.Items("ChkDateF").Specific.VALUE = Format(Now, "YYYYMM01")
    oForm01.Items("ChkDateT").Specific.VALUE = Format(Now, "YYYYMMDD")
    
    oForm01.Visible = True
    Set oXmlDoc01 = Nothing

    Exit Sub
LoadForm_Error:
    oForm01.Update
    oForm01.Freeze False
    Set oXmlDoc01 = Nothing
    Set oForm01 = Nothing
    Sbo_Application.SetStatusBarMessage "Form_Load Error:" & Err.Description, bmt_Short, True
End Sub

'/메트릭스 Row추가
Sub PS_QM430_Add_MatrixRow01(ByVal oRow As Long, Optional RowIserted As Boolean)
On Error GoTo PS_QM430_Add_MatrixRow01_Error
    If RowIserted = False Then '//행추가여부
        oDS_PS_QM430L.InsertRecord (oRow)
    End If
    
    oMat01.AddRow
    oDS_PS_QM430L.Offset = oRow
    oDS_PS_QM430L.setValue "U_LineNum", oRow, oRow + 1
    
    oMat01.LoadFromDataSource
    Exit Sub
'////////////////////////////////////////////////////////////////////////////////////////////////////////////////
PS_QM430_Add_MatrixRow01_Error:
    MDC_Com.MDC_GF_Message "PS_QM430_Add_MatrixRow01_Error:" & Err.Number & " - " & Err.Description, "E"
End Sub

Public Sub PS_QM430_MTX01()
'******************************************************************************
'Function ID : PS_QM430_MTX01()
'해당모듈 : PS_QM430
'기능 : 데이터 조회
'인수 : 없음
'반환값 : 없음
'특이사항 : 없음
'******************************************************************************
On Error GoTo PS_QM430_MTX01_Error

    Dim i As Integer
    Dim sQry As String
    
    Dim ErrNum As Integer
    
    Dim oRecordSet01 As SAPbobsCOM.Recordset
    Set oRecordSet01 = Sbo_Company.GetBusinessObject(BoRecordset)
    
    Dim oRecordset02 As SAPbobsCOM.Recordset
    Set oRecordset02 = Sbo_Company.GetBusinessObject(BoRecordset)

    Dim ChkDateF As String '검사일자(시작)
    Dim ChkDateT As String '검사일자(종료)
    Dim CardCode As String '납품처
    Dim ItemCode As String '작번
    Dim DrawNo As String '도면번호
    
    ChkDateF = Trim(oForm01.Items("ChkDateF").Specific.VALUE) '검사일자(시작)
    ChkDateT = Trim(oForm01.Items("ChkDateT").Specific.VALUE) '검사일자(종료)
    CardCode = Trim(oForm01.Items("CardCode").Specific.VALUE) '납품처
    ItemCode = Trim(oForm01.Items("ItemCode").Specific.VALUE) '작번
    DrawNo = Trim(oForm01.Items("DrawNo").Specific.VALUE) '도면번호
    
    Dim ProgBar01 As SAPbouiCOM.ProgressBar
    Set ProgBar01 = Sbo_Application.StatusBar.CreateProgressBar("조회시작!", oRecordSet01.RecordCount, False)
    
    Call oForm01.Freeze(True)
    
    sQry = "         EXEC [PS_QM430_01] '"
    sQry = sQry & ChkDateF & "','" '검사일자(시작)
    sQry = sQry & ChkDateT & "','" '검사일자(종료)
    sQry = sQry & CardCode & "','" '납품처
    sQry = sQry & ItemCode & "','" '작번
    sQry = sQry & DrawNo & "'" '도면번호
    
    Call oRecordSet01.DoQuery(sQry)
    
    oMat01.Clear
    oDS_PS_QM430L.Clear
    oMat01.FlushToDataSource
    oMat01.LoadFromDataSource
    
    If (oRecordSet01.RecordCount = 0) Then
    
        ErrNum = 1
        
        oForm01.Mode = fm_ADD_MODE
        
        Call PS_QM430_Add_MatrixRow01(0, True)
        GoTo PS_QM430_MTX01_Error
        
        Exit Sub
    End If
    
    For i = 0 To oRecordSet01.RecordCount - 1
    
        If i + 1 > oDS_PS_QM430L.Size Then
            oDS_PS_QM430L.InsertRecord (i)
        End If
    
        oMat01.AddRow
        oDS_PS_QM430L.Offset = i
    
        Call oDS_PS_QM430L.setValue("U_LineNum", i, i + 1)
        Call oDS_PS_QM430L.setValue("U_ColReg01", i, Trim(oRecordSet01.Fields("Check").VALUE)) '선택
        Call oDS_PS_QM430L.setValue("U_ColReg02", i, Trim(oRecordSet01.Fields("QM410No").VALUE)) '품질검사번호
        Call oDS_PS_QM430L.setValue("U_ColReg03", i, Trim(oRecordSet01.Fields("CardCode").VALUE)) '납품처코드
        Call oDS_PS_QM430L.setValue("U_ColReg04", i, Trim(oRecordSet01.Fields("CardName").VALUE)) '납품처명
        Call oDS_PS_QM430L.setValue("U_ColReg05", i, Trim(oRecordSet01.Fields("ItemCode").VALUE)) '작번
        Call oDS_PS_QM430L.setValue("U_ColReg06", i, Trim(oRecordSet01.Fields("ItemName").VALUE)) '품명
        Call oDS_PS_QM430L.setValue("U_ColDt01", i, Format(Trim(oRecordSet01.Fields("ChkDate").VALUE), "YYYYMMDD")) '검사일자
        Call oDS_PS_QM430L.setValue("U_ColReg08", i, Trim(oRecordSet01.Fields("DrawNo").VALUE)) '도면번호
        
        oRecordSet01.MoveNext
        ProgBar01.VALUE = ProgBar01.VALUE + 1
        ProgBar01.Text = ProgBar01.VALUE & "/" & oRecordSet01.RecordCount & "건 조회중...!"
        
    Next
    
    oMat01.LoadFromDataSource
    oMat01.AutoResizeColumns
    
    ProgBar01.Stop
    
    Call oForm01.Freeze(False)
  
    Set ProgBar01 = Nothing
    Set oRecordSet01 = Nothing
    Set oRecordset02 = Nothing
    Exit Sub
'////////////////////////////////////////////////////////////////////////////////////////////////////////////////
PS_QM430_MTX01_Error:
'    ProgBar01.Stop
    Call oForm01.Freeze(False)
    Set ProgBar01 = Nothing
    Set oRecordSet01 = Nothing
    Set oRecordset02 = Nothing
    
    If ErrNum = 1 Then
        Call MDC_Com.MDC_GF_Message("조회 결과가 없습니다. 확인하세요.", "W")
    Else
        Call MDC_Com.MDC_GF_Message("PS_QM430_MTX01_Error:" & Err.Number & " - " & Err.Description, "E")
    End If
End Sub

