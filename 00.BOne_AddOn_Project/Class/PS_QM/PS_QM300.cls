Private Sub Raise_EVENT_VALIDATE(ByRef FormUID, ByRef pval As SAPbouiCOM.ItemEvent, ByRef BubbleEvent As Boolean)
On Error GoTo Raise_EVENT_VALIDATE_Error

    Dim Query01 As String
    Dim RecordSet01 As SAPbobsCOM.Recordset
    Set RecordSet01 = Sbo_Company.GetBusinessObject(BoRecordset)
    
    Dim PS_PH030() As String

    Call oForm01.Freeze(True)
    If pval.BeforeAction = True Then
        If pval.ItemChanged = True Then

            If (pval.ItemUID = "Mat01") Then
            
                If pval.ColUID = "OrdMgNum" Then '작지문서
                
                    oMat01.FlushToDataSource
                    
                    Call oDS_PS_QM300L.setValue("U_" & pval.ColUID, pval.Row - 1, oMat01.Columns(pval.ColUID).Cells(pval.Row).Specific.VALUE)
                    
'                    Call oDS_PS_QM300L.setValue("U_CntcName", pval.Row - 1, MDC_GetData.Get_ReData("U_FULLNAME", "U_MSTCOD", "[OHEM]", "'" & oMat01.Columns(pval.ColUID).Cells(pval.Row).Specific.VALUE & "'")) '성명
'                    Call oDS_PS_QM300L.setValue("U_TeamCode", pval.Row - 1, MDC_GetData.Get_ReData("U_TeamCode", "U_MSTCOD", "[OHEM]", "'" & oMat01.Columns(pval.ColUID).Cells(pval.Row).Specific.VALUE & "'")) '소속팀
'                    Call oDS_PS_QM300L.setValue("U_RspCode", pval.Row - 1, MDC_GetData.Get_ReData("U_RspCode", "U_MSTCOD", "[OHEM]", "'" & oMat01.Columns(pval.ColUID).Cells(pval.Row).Specific.VALUE & "'")) '소속담당

                    PS_PH030 = Split(oMat01.Columns(pval.ColUID).Cells(pval.Row).Specific.VALUE, "-")
                    
                    '중량구하기
                    Query01 = "EXEC PS_QM300_90 " & PS_PH030(0) & "," & PS_PH030(1)
                    Call RecordSet01.DoQuery(Query01)

                    Call oDS_PS_QM300L.setValue("U_ItemCode", pval.Row - 1, MDC_GetData.Get_ReData("U_ItemCode", "DocEntry", "[@PS_PP030H]", "'" & PS_PH030(0) & "'")) '제품코드
                    Call oDS_PS_QM300L.setValue("U_ItemName", pval.Row - 1, MDC_GetData.Get_ReData("U_ItemName", "DocEntry", "[@PS_PP030H]", "'" & PS_PH030(0) & "'")) '제품명
                    Call oDS_PS_QM300L.setValue("U_OrdNum", pval.Row - 1, MDC_GetData.Get_ReData("U_OrdNum", "DocEntry", "[@PS_PP030H]", "'" & PS_PH030(0) & "'")) '작지Main(코일No)
                    Call oDS_PS_QM300L.setValue("U_BatchNum", pval.Row - 1, MDC_GetData.Get_ReData("TOP 1 U_BatchNum", "DocEntry", "[@PS_PP030L]", "'" & PS_PH030(0) & "'")) '온산LotNo
                    Call oDS_PS_QM300L.setValue("U_OrdWt", pval.Row - 1, RecordSet01.Fields(0).VALUE) '중량
                    Call oDS_PS_QM300L.setValue("U_RealWt", pval.Row - 1, RecordSet01.Fields(0).VALUE) '실중량
                    Call oDS_PS_QM300L.setValue("U_PP030HNo", pval.Row - 1, PS_PH030(0)) '작지문서번호
                    Call oDS_PS_QM300L.setValue("U_PP030MNo", pval.Row - 1, PS_PH030(1)) '작지라인번호

                    If oMat01.RowCount = pval.Row And Trim(oDS_PS_QM300L.GetValue("U_" & pval.ColUID, pval.Row - 1)) <> "" Then
                        PS_QM300_AddMatrixRow (pval.Row)
                    End If
                    
                    oMat01.LoadFromDataSource
                    
